{
  "name": "univ-fs-box",
  "version": "0.0.1",
  "description": "The universal Filesystem API for Box",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "keywords": [
    "fs",
    "filesystem",
    "storage",
    "isomorphic",
    "universal",
    "box"
  ],
  "scripts": {
    "clean": "rimraf ./lib",
    "compile": "tsc",
    "build": "npm-run-all clean compile",
    "npm-version": "npm version patch",
    "npm-publish": "npm publish",
    "release": "npm-run-all clean compile npm-version npm-publish",
    "test": "jest --runInBand"
  },
  "jest": {
    "testTimeout": 15000,
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "<rootDir>/src/__tests__/**/*.spec.+(ts|tsx|js)"
    ],
    "preset": "ts-jest"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "box-node-sdk": "^2.1.0",
    "tslib": "^2.3.1",
    "univ-fs": "^0.6.35"
  },
  "author": "Yoshihiro Seki",
  "license": "MIT"
}
